@    using Cvl.DynamicForms.Core.Models
@    using Cvl.DynamicForms.WebApp.Components.FormElements

@if (Model != null)
{
    if (Model is ItemsControl itemsControl)
    {
        <ItemsControlView Model="itemsControl"></ItemsControlView>
    }
    else if (Model is ContentControl contentControl)
    {
        <ContentControlView Model="contentControl"></ContentControlView>
    }
}


@code {
    [Parameter]
    public Control? Model { get; set; }
}
