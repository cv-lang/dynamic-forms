@model Cvl.DynamicForms.Model.ViewModel.TreeListViewModel
@{
    <div class="container">
        <button type="button" class="btn btn-primary" onclick="getData(1, 'Logger')">Get</button>
        <ul id="target">

        </ul>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js" type="text/javascript"></script>
    <script type="text/javascript">
        function getData(id, type) {
            $.ajax({
                type: "get",
                url: "/DynamicForms/TreeList/Node?Id=" + id + "&type=" + type,
                dataType: "html",
                contentType: "text/html",
                success: function (data) {
                    $('#target').append(data);
                },
                failure: function (response) {
                    alert("niefajne");
                }
            })
        }

    </script>
}