@page
@model Cvl.DynamicForms.Areas.DynamicForms.Pages.TreeList.NodeModel
@* Strona odpowiedzialna za doczytywanie dynamiczne dzieci *@
@if (Model.Region is Cvl.DynamicForms.Model.ViewModel.TreeListViewModel)
{
    @await Component.InvokeAsync("TreeNodeView", Model.Region)
}
else if (Model.Region is Cvl.DynamicForms.Model.PropertyGridVM)
{
    @await Component.InvokeAsync("PropertyView", Model.Region)
}
